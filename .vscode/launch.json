{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GPSRCmdGen",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-gpsr",
            "program": "${workspaceFolder}/GPSRCmdGen/bin/Debug/net5.0/GPSRCmdGen.dll",
            "args": [
                "interactive",
                "--seed", "22",
                "--grammars",
                "../CommonFiles/CommonRules.txt",
                "../CommonFiles/EGPSRGrammar.txt"
            ],
            "cwd": "${workspaceFolder}/GPSRCmdGen",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}